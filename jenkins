pipeline {
    agent any
    
    stages {
        stage('build') {
            steps {
                echo 'build Java program...'
            }
        }

        stage('Compile') {
            steps {
                echo 'Compiling Java program...'
            }
        }

        stage('Run') {
            steps {
                echo 'Running Java program...'
            }
        }
    }
}
