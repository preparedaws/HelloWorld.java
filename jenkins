pipeline {
    agent any
    
    stages {
        stage('Clone Repository') {
            steps {
                echo 'Cloning GitHub Repository from main branch...'
                git branch: 'main', url: 'https://github.com/preparedaws/HelloWorld.java.git'
            }
        }

        stage('Compile') {
            steps {
                echo 'Compiling Java program...'
            }
        }

        stage('Run') {
            steps {
                echo 'Running Java program...'
            }
        }
    }
}
